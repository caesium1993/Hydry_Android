<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hydry.AfterShake_2Activity">

    <TextView
        android:id="@+id/restaurant2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:text="The Meat &amp; Wine Co"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/restaurantAddress2"
        android:layout_width="236dp"
        android:layout_height="47dp"
        android:text="Freshwater Place, 3 Freshwater Place, Southbank, Melbourne"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/restaurant2"
        tools:layout_editor_absoluteX="151dp" />

    <TextView
        android:id="@+id/restaurantDescription2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rustic steaks within a sleek and modern ambience."
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/restaurantAddress2" />

    <ImageView
        android:id="@+id/meatwineco1"
        android:layout_width="161dp"
        android:layout_height="124dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:src="@drawable/meatwineco1"
        android:layout_marginTop="85dp"
        app:layout_constraintTop_toBottomOf="@+id/meatwineco4" />

    <ImageView
        android:id="@+id/meatwineco3"
        android:layout_width="160dp"
        android:layout_height="86dp"
        android:src="@drawable/meatwineco2"
        android:layout_marginLeft="208dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="85dp"
        app:layout_constraintTop_toBottomOf="@+id/meatwineco4" />

    <ImageView
        android:id="@+id/meatwineco4"
        android:layout_width="194dp"
        android:layout_height="100dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:src="@drawable/meatwineco3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/restaurantDescription2" />

    <ImageView
        android:id="@+id/meatwineco2"
        android:layout_width="146dp"
        android:layout_height="94dp"
        android:src="@drawable/meatwineco4"
        android:layout_marginLeft="222dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/restaurantDescription2" />
</android.support.constraint.ConstraintLayout>
