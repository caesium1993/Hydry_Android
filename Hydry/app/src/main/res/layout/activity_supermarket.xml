<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5F5169E1"
    tools:context="com.example.hydry.SupermarketActivity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="250dp"
        android:layout_height="42dp"
        android:text="Fresh Food"
        android:textSize="65px"
        android:textColor="#FFFFF0"
        android:typeface="monospace"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="113dp"
        android:layout_height="46dp"
        android:src="@drawable/coles"
        android:onClick="Tocoles"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="68dp"
        android:layout_height="59dp"
        android:src="@drawable/woolworths"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintRight_toLeftOf="@+id/imageButton6"
        android:layout_marginRight="27dp"
        android:onClick="Toworth"
        app:layout_constraintLeft_toRightOf="@+id/imageButton2"
        android:layout_marginLeft="30dp"
        app:layout_constraintHorizontal_bias="0.75" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="91dp"
        android:layout_height="63dp"
        android:src="@drawable/vm"
        android:onClick="ToVM"
        android:layout_marginRight="37dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="17dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:typeface="monospace"
        android:text="Business"
        android:textSize="65px"
        android:textColor="#FFFFF0"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="39dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton2" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="83dp"
        android:layout_height="66dp"
        android:src="@drawable/jbhifi"
        android:onClick="Tojibihifi"
        android:layout_marginLeft="46dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="157dp"
        android:layout_height="39dp"
        android:src="@drawable/officeworks"
        android:onClick="Toofficework"
        app:layout_constraintLeft_toRightOf="@+id/imageButton7"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        android:layout_marginTop="92dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton6" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="65px"
        android:text="Daily Product"
        android:textColor="#FFFFF0"
        android:typeface="monospace"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="87dp"
        android:layout_height="77dp"
        android:src="@drawable/bigw"
        android:layout_marginLeft="28dp"
        android:onClick="Tobigw"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="83dp"
        android:layout_height="82dp"
        android:src="@drawable/kmart"
        android:onClick="Tokmart"
        app:layout_constraintLeft_toRightOf="@+id/imageButton9"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="82dp"
        android:layout_height="99dp"
        android:src="@drawable/target"
        android:onClick="Totarget"
        app:layout_constraintLeft_toRightOf="@+id/imageButton10"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.442"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />


</android.support.constraint.ConstraintLayout>
